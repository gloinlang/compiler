fn main() -> i32 {
    def mut result: i32 = 0;
    
    // Test switch with variable
    def x: i32 = 3;
    switch x {
        case 1:
            result = result + 10;
        case 2:
            result = result + 20;
        case 3:
            result = result + 30;
        default:
            result = result + 100;
    }
    
    // Test switch with expression
    switch result + 1 {
        case 31:
            result = result * 2;
        default:
            result = result + 1;
    }
    
    return result;
}
