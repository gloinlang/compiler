import "@std"

fn test_mutability() -> i32 {
    def x: i32 = 10;           // immutable
    def mut y: i32 = 20;       // mutable
    def z: i32 = x + y;        // immutable
    def mut result: i32 = z * 2; // mutable
    
    return result;
}

fn main() -> i32 {
    def final_result: i32 = test_mutability();
    return final_result;
}
