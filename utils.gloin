import "@std"

def main() -> i32 {
    def x: i32 = 10;
    def y: i32 = 20;
    
    // Simple max/min operations inline
    def mut maximum: i32 = x;
    if y > x {
        maximum = y;
    }
    
    def mut minimum: i32 = x;
    if y < x {
        minimum = y;
    }
    
    std.print("Utility functions demo:\n");
    std.print("max(10, 20) = ");
    std.println(std.to_string(maximum));
    std.print("min(10, 20) = ");
    std.println(std.to_string(minimum));
    
    return 0;
}
