import "@std"

def main() -> i32 {
    def a: i32 = 5;
    def b: i32 = 15;
    
    // Simple addition function inline
    def result: i32 = a + b;
    
    std.print("Simple fibonacci demo: ");
    std.println(std.to_string(result));
    
    def n: i32 = 10;
    if n > 5 {
        std.println("Number is greater than 5");
    }
    
    return 0;
}
